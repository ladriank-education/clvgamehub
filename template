const db = require('../db');
var DataTypes = require("sequelize").DataTypes;
const { Op } = require("sequelize");
const <Models> = require('../models/<Models>')(db, db.Sequelize);

/**
*
* @return <Models>[]
*
**/
const getAll = async () => {
	try { // example code
		const r = await <Models>.findAll();
		return (r.length > 0) ? r : null;
	} catch (e) {
		throw e;
	}
};

module.exports = {
	getAll,
};